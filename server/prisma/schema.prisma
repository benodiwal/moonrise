generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          Int        @id @default(autoincrement())
  address     String     @unique
  thumbnails  Thumbnail[]
}

model Thumbnail {
  id         Int       @id @default(autoincrement())
  userId      Int
  url         String
  user        User     @relation(fields: [userId], references: [id])
}
